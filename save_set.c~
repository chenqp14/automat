#include "read_from_file.h"
#include <stdio.h>
#include <stdlib.h>

void set_to_file ( FILE *out, matrix_t *new_matrix){
  int i, j;
  if (m == NULL) {
    fprintf (out, "Matrix is NULL\n");
    return;
  }

  fprintf (out, "%d %d\n", m->rn, m->cn);
  for (i = 0; i < m->rn; i++) {
    for (j = 0; j < m->cn - 1; j++)
      fprintf (out, "%d ", m->e[i * m->cn + j]);
    fprintf (out, "%d\n", m->e[i * m->cn + j]);
  }
}
